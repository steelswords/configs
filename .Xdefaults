URxvt*font: xft:DejaVuSansMono Nerd Font Mono:size=11,xft:Lucida Sans Unicode,xft:Noto Sans Mono Regular
URxvt*boldFont: xft:DejaVuSansMono Nerd Font Mono:size=11

! Works really well!!
!URxvt*font: xft:Source Code Variable:size=11:antialias=true, DejaVuSansMono Nerd Font Mono:size=9:antialias=true
!URxvt*boldFont: xft:Source Code Variable:bold:size=11:antialias=true, DejaVuSansMono Nerd Font Mono:size=9:antialias=true:bold


!URxvt.letterSpace: -1


URxvt*scrollBar: false

! special
*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6

! black
*.color0:       #1d1f21
*.color8:       #969896

! red
*.color1:       #cc342b
*.color9:       #cc342b

! green
*.color2:       #198844
*.color10:      #198844

! yellow
*.color3:       #fba922
*.color11:      #fba922

! blue
*.color4:       #3971ed
*.color12:      #3971ed

! magenta
*.color5:       #a36ac7
*.color13:      #a36ac7

! cyan
*.color6:       #3971ed
*.color14:      #3971ed

! white
*.color7:       #c5c8c6
*.color15:      #ffffff

